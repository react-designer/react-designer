{
  "name": "react-designer",
  "version": "1.1.1",
  "description": "Easy to configure, lightweight, editable vector graphics in your react components.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "node server.js",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.js",
    "prepare": "babel -d lib/ src/",
    "build": "npm run prepare",
    "install": "npm run clean && npm run build",
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fatiherikli/react-designer"
  },
  "keywords": [
    "react",
    "reactjs"
  ],
  "author": "Fatih Erikli <fatiherikli@gmail.com> (http://fatiherikli.com)",
  "license": "MIT",
  "homepage": "https://github.com/react-designer/react-designer",
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "css-loader": "^2.1.1",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^3.0.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-autocomplete": "1.8.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^2.7.1",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {},
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react-color": "^2.18.1",
    "react-hotkeys": "^1.1.4",
    "react-portal": "^4.2.1",
    "superagent": "^5.2.2",
    "webfontloader": "^1.6.28"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
